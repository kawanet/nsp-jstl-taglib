{
  "name": "nsp-jstl-taglib",
  "description": "NSP taglib for JSTL Jakarta Standard Tag Library",
  "version": "0.0.1",
  "author": "@kawanet",
  "bugs": {
    "url": "https://github.com/kawanet/nsp-jstl-taglib/issues"
  },
  "dependencies": {
    "telesy": "^1.1.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.4",
    "mocha": "^10.2.0",
    "nsp-server-pages": "^0.1.1",
    "typescript": "^5.1.6"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./esm/src/index.js"
      },
      "require": "./cjs/src/index.js"
    }
  },
  "files": [
    "cjs/package.json",
    "cjs/src/**/*.js",
    "esm/src/**/*.js",
    "package.json",
    "types/*.d.ts"
  ],
  "homepage": "https://github.com/kawanet/nsp-jstl-taglib#readme",
  "keywords": [
    "JSP",
    "JSTL",
    "Tomcat",
    "taglib"
  ],
  "license": "Apache-2.0",
  "main": "./esm/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/nsp-jstl-taglib.git"
  },
  "scripts": {
    "build": "make clean all",
    "build-cjs": "tsc -p tsconfig-cjs.json",
    "build-esm": "tsc -p tsconfig.json",
    "fixpack": "fixpack",
    "prepack": "npm run build && npm run test",
    "test": "make test",
    "test-cjs": "mocha cjs/test",
    "test-esm": "mocha esm/test"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./types/index.d.ts"
}
